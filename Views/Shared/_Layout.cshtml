<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - IronOCRDemo</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/IronOCRDemo.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">IronOCRDemo</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Create">Create</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="BarcodeQR">Barcode</a>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-dark" href="#" id="rsDocumentDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Document
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="rsDocumentDropdown">
                                <li><a class="dropdown-item" asp-controller="Home" asp-action="RSDocument">RSDocument</a></li>
                                 <li><a class="dropdown-item" asp-controller="Home" asp-action="ReadTable">ReadTable </a></li>
                                <li><a class="dropdown-item" asp-controller="Home" asp-action="ReadLicensePlate">ReadLicensePlate</a></li>
                                <li><a class="dropdown-item" asp-controller="Home" asp-action="ReadPassport">ReadPassport</a></li>
                                <li><a class="dropdown-item" asp-controller="Home" asp-action="ReadPhoto">Read Photo</a></li>
                                <li><a class="dropdown-item" asp-controller="Home" asp-action="ReadScreen">Read Screenshot</a></li>

                            </ul>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Tesseract" asp-action="ReadPdf">ReadPdf </a>
                        </li>
                        

                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-dark" href="#" id="fDocumentDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Formats
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="fDocumentDropdown">
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Formats" asp-action="InputClass">InputClass </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Formats" asp-action="RegionOcr">RegionOcr </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Formats" asp-action="OcrDraw">Ocr Drawing </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Formats" asp-action="OcrMultiPageTiff">Ocr MultiPage Tiff </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2025 - IronOCRDemo - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
